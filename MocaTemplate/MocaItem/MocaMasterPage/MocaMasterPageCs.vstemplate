    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <References>
      <Reference>
        <Assembly>Moca</Assembly>
      </Reference>
      <Reference>
        <Assembly>MocaWeb</Assembly>
      </Reference>
    </References>

    <ProjectItem SubType="" TargetFileName="$fileinputname$.Master" ReplaceParameters="true">Site1.Master</ProjectItem>
    <ProjectItem SubType="" TargetFileName="$fileinputname$.Master.designer.cs" ReplaceParameters="true">Site1.Master.designer.cs</ProjectItem>
    <ProjectItem SubType="ASPXCodebehind" TargetFileName="$fileinputname$.Master.cs" ReplaceParameters="true">Site1.Master.cs</ProjectItem>
  </TemplateContent>
</VSTemplate>
